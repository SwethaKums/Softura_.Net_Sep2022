@model IEnumerable<EmployeeApp.Models.EmployeeDetails>
@{
    Layout = "_Layout";
    ViewData["Title"] = "AllEmpInfo";
}
<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model=>model.EmpId)</th>
        <th>@Html.DisplayNameFor(model=>model.EmpFirstName)</th>
        <th>@Html.DisplayNameFor(model=>model.EmpLastName)</th>
        <th>@Html.DisplayNameFor(model=>model.EmpGender)</th>
    </tr>
    @foreach(var i in Model){
        <tr>
            <td>@i.EmpId</td>
            <td>@i.EmpFirstName</td>
            <td>@i.EmpLastName</td>
            <td>@i.EmpGender</td>
        </tr>
    }
</table>
<a asp-action="insert">Insert</a>
